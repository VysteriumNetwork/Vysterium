---
import NavBar from "../../components/generalmath/NavBar.astro";
---
<html lang="en">
<head>
  <title>General Mathematics</title>
  <!-- Meta/Info Attributes -->
  <link rel = "icon" href = "./img/icons/math.ico" type = "image/x-icon">
  <meta charset="utf-8">
  <link rel="stylesheet" href="./loader.css">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- Boxicons CDN Link -->
  <link href='https://unpkg.com/boxicons@2.1.2/css/boxicons.min.css' rel='stylesheet'>
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
  <!-- Link to CSS Stylesheet -->
  <link rel="stylesheet" href="./main.css">
</head>
<div class="background_content">
<body>
 <!--- Sidebar / Navigation Bar-->
 <NavBar/>
 <div class="home_content">
    <h1> General Mathematics </h1>
    <div style="text-align:center;margin-left: 1%;">
    <form>
    <button class="web online search imagebutton" style="background-image: url(./img/sites/chatroom.png); background-repeat: none;" onclick="window.location.href='./integrations/chatroom/chatroom.html'; timer()">Chatroom</button>
    <button class="web online search imagebutton" style="background-image: url(./img/sites/geforce.png); background-repeat: none;" onclick="location.href=selfindex$config.prefix + selfindex$config.encodeUrl('https://play.geforcenow.com'); timer()">Geforce Now</button>
