    </div>
</div>

    <script is:inline src="/script/bundle.js"></script>
    <script is:inline src="/script/config.js"></script>
    <script is:inline src="./scripts/index.js"></script>
    <script is:inline src="./scripts/settings.js"></script>

  </body>
</html>
